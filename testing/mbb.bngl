begin model

begin parameters

	kon		2
	koff	1
	
	f		10
	A_init	1*f
	B_init	1*f
	
end parameters

begin molecule types

	A(b)
	B(a)
end molecule types

begin seed species
	A(b)	A_init
	B(a) 	B_init
end seed species

begin observables
	Molecules	A_free	A(b)
	Molecules	B_free 	B(a)
	Molecules	Complex	A(b!1).B(a!1)
end observables

begin reaction rules
	A(b) + B(a) <-> A(b!1).B(a!1) kon, koff
end reaction rules

end model

### Actions ###
# construct reaction network
generate_network({overwrite=>1})

# Equilibration..
simulate({method=>"ode",suffix=>"equil",t_end=>1000,n_steps=>1000})

